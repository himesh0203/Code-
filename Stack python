class Stack:
    def __init__(self):
        self.items = []

    def is_empty(self):
        return self.items == []

    def push(self, item):
        self.items.append(item)

    def pop(self):
        if not self.is_empty():
            return self.items.pop()
        else:
            return "Stack is empty"

    def peek(self):
        if not self.is_empty():
            return self.items[-1]
        else:
            return "Stack is empty"

# Example usage
s = Stack()
s.push(1)
s.push(2)
s.push(3)
print("Stack after push operations:", s.items)

print("Top element is:", s.peek())

s.pop()
print("Stack after pop operation:", s.items)
